post 42.0 to "sensor1";

POST 42.1 TO "sensor2";

post 43.0 TO "sensor1" where "serial" is "A001";

get min from "sensor1" SINCE 1m;

get from "sensor1" SINCE 24h;

get from "sensor1" SINCE 1d;

get from "sensor1,sensor2" where "serial" is "A001" since 1000s;

get from "sensor2" last 10;

get count from "sensor1,sensor2" where "serial" is "A001" since 10sec;

get from "sensor1" range 10days to 0days;

delete from "sensor2" range 10m to 5m;

delete from "sensor1" where "serial" is "A001" range 5s to 0s;
